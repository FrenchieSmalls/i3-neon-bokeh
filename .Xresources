! ------------------------------------------------------------------------------
! ROFI Configuration
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #9915262c, #9915262c, #38acd6
rofi.color-normal: #FF15262c, #ECB3FF, #FF15262c, #ECB3FF, #182e3e
rofi.color-active: #FF15262c, #38acd6, #FF15262c, #38acd6, #15262c
rofi.color-urgent: #FF15262c, #dc322f, #FF15262c, #dc322f, #15262c


rofi.hide-scrollbar: 	true
rofi.font:  			Meslo LG S DZ 13
rofi.separator-style:   solid
rofi.fullscreen:		false
rofi.fixed-num-lines:	true

rofi.bw: 			0

rofi.width: 		1366

rofi.lines:         10

rofi.line-margin:   0

rofi.eh:			1

rofi.location: 		2

rofi.columns:		3

rofi.sidebar-mode:  true

rofi.padding:		226



! ------------------------------------------------------------------------------
! XTerm configuration for ranger
! ------------------------------------------------------------------------------

XTerm*faceName: Meslo LG S DZ
xterm*faceSize: 10
xterm*vt100*geometry: 80x60
xterm*saveLines: 16384
xterm*loginShell: true
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName: xterm-color
xterm*eightBitInput: false

